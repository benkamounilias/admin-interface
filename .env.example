# Variables d'environnement pour l'interface d'administration PPAM

# URL de base de l'API backend
VITE_API_BASE_URL=http://localhost:8080/api

# Clé de cryptage pour le stockage local (optionnel)
VITE_STORAGE_KEY=ppam_admin_storage

# Mode de développement (development, production)
VITE_MODE=development

# Timeout pour les requêtes API (en millisecondes)
VITE_API_TIMEOUT=30000

# URL de redirection après logout (optionnel)
VITE_LOGOUT_REDIRECT_URL=/login
